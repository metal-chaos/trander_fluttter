    "version": "0.3.4",
    "configurations": [
        {
            "name": "Run dev",
            "request": "launch",
            "type": "dart",
            "args": [
                "--dart-define=FLAVOR=dev"
            ]
        },
        {
            "name": "Run stg",
            "request": "launch",
            "type": "dart",
            "args": [
                "--dart-define=FLAVOR=stg"
            ]
        },
        {
            "name": "Run prod",
            "request": "launch",
            "type": "dart",
            "args": [
                "--dart-define=FLAVOR=prod"
            ]
        },
        {
            "name": "Run prod (Release)",
            "request": "launch",
            "type": "dart",
            "args": [
                "appbundle",
                "--release",
                "--dart-define=FLAVOR=prod"
            ]
        }
    ]
}